  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "scripts": {
    "build": "webpack --config ./webpack/webpack.prod.js",
    "start": "webpack-dev-server --config ./webpack/webpack.dev.js --open",
    "clean": "tsc -b --clean && rm -rf dist",
    "serverless": "serverless",
    "aws:profile:set": "serverless config credentials --provider aws --key $AWS_ACCESS_KEY_ID --secret $AWS_SECRET_ACCESS_KEY --profile picnic -o",
    "deploy:staging": "yarn deploy:staging:stack && yarn deploy:staging:website && yarn deploy:staging:info",
    "deploy:staging:stack": "serverless deploy --verbose --stage staging",
    "deploy:staging:website": "serverless client deploy --verbose --stage staging --no-confirm",
    "deploy:staging:info": "serverless info --verbose --stage staging",
    "deploy:prod": "yarn deploy:prod:stack && yarn deploy:prod:website && yarn deploy:prod:info",
    "deploy:prod:stack": "serverless deploy --verbose --stage production",
    "deploy:prod:website": "serverless client deploy --verbose --stage production --no-confirm",
    "deploy:prod:info": "serverless info --verbose --stage production"
  }
}
